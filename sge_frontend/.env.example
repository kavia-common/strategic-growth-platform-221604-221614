# Copy this file to .env and fill in values for local development.
# React requires variables to be prefixed with REACT_APP_

# Backend base URL (preferred). Example: http://localhost:3001
REACT_APP_API_URL=

# Alternative backend base URL variables (fallbacks if API_URL not set)
REACT_APP_BACKEND_URL=
REACT_APP_API_BASE=

# Frontend site URL (used for Supabase email redirect)
# Defaults to window.location.origin if not set
REACT_APP_SITE_URL=http://localhost:3000

# Supabase settings (required)
REACT_APP_SUPABASE_URL=
REACT_APP_SUPABASE_ANON_KEY=

# Optional publishable default key if used in other parts of app
REACT_APP_SUPABASE_PUBLISHABLE_DEFAULT_KEY=

# Other optional runtime flags
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_LOG_LEVEL=debug

# Websocket URL if applicable (not used directly in this app yet)
REACT_APP_WS_URL=
